{
    "name": "web-auth/webauthn-symfony-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "FIDO2/Webauthn Security Bundle For Symfony",
    "keywords": ["FIDO", "FIDO2", "webauthn"],
    "homepage": "https://github.com/web-auth",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky"
        },
        {
            "name": "All contributors",
            "homepage": "https://github.com/web-auth/webauthn-symfony-bundle/contributors"
        }
    ],
    "require": {
        "spomky-labs/cbor-bundle": "^2.0",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "web-auth/webauthn-lib": "self.version",
        "web-token/jwt-signature": "^2.0.3"
    },
    "require-dev": {
        "nyholm/psr7": "^1.1",
        "fzaninotto/faker": "^1.8",
        "symfony/psr-http-message-bridge": "^2.0",
        "symfony/security-bundle": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Webauthn\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webauthn\\Bundle\\Tests\\Functional\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "v1.0": "1.0.x-dev",
            "v1.1": "1.1.x-dev",
            "v1.2": "1.2.x-dev",
            "v2.0": "2.0.x-dev",
            "v2.1": "2.1.x-dev",
            "v3.0": "3.0.x-dev",
            "v3.1": "3.1.x-dev",
            "v3.2": "3.2.x-dev"
        }
    }
}
